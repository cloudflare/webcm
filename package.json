{
  "name": "webcm",
  "version": "1.0.0",
  "description": "Demonstrative implementation of a web-based manager for utilising Managed Components",
  "repository": {
    "type": "git",
    "url": "https://github.com/cloudflare/webcm"
  },
  "author": "Cloudflare Managed Components Team <managedcomponents@cloudflare.com> (https://blog.cloudflare.com/zaraz-open-source-managed-components-and-webcm/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cloudflare/webcm"
  },
  "keywords": [
    "webcm",
    "managed-components",
    "cloudflare"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "bin": {
    "webcm": "./bin/webcm.js"
  },
  "main": "index.ts",
  "scripts": {
    "dev": "TS_NODE_FILES=true nodemon --exec 'node --experimental-network-imports --loader ts-node/esm' index.ts",
    "lint": "eslint --ext .ts,.js, lib",
    "lint:fix": "eslint --ext .ts,.js, lib --fix",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --globals",
    "test:dev": "vitest --globals",
    "buildcheck": "run-s typecheck test lint"
  },
  "dependencies": {
    "cookies": "^0.8.0",
    "express": "^4.18.1",
    "http-proxy-middleware": "^2.0.6",
    "jsdom": "^19.0.0",
    "pacote": "^13.6.0",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@managed-components/types": "^1.0.0",
    "@types/cookies": "^0.7.7",
    "@types/express": "^4.17.13",
    "@types/jsdom": "^16.2.14",
    "@types/node": "^17.0.36",
    "@types/pacote": "^11.1.4",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "nodemon": "^2.0.16",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "ts-node": "^10.8.0",
    "typescript": "^4.8.0-dev.20220530",
    "vitest": "^0.13.0"
  }
}
